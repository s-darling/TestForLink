<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ebxbnd="urn:ebx-schemas:binding_1.0" xmlns:fmt="urn:ebx-schemas:format_1.0" xmlns:osd="urn:ebx-schemas:common_1.0">
	<xs:annotation>
		<xs:appinfo>
			<osd:resourceBundle source="com.orchestranetworks.i18n.xsdSchema.workflow_types" />
		</xs:appinfo>
	</xs:annotation>
	<xs:import namespace="urn:ebx-schemas:common_1.0" schemaLocation="../common_1.0.xsd" />
	<!--
		******************************************************************************
		Workflow definition types
		******************************************************************************
	-->
	<xs:simpleType name="positiveInteger">
  		<xs:restriction base="xs:integer">
   			<xs:minInclusive value="0"/>
  		</xs:restriction>
 	</xs:simpleType>
	<xs:complexType name="ProcessDefinition">
		<xs:sequence>
			<xs:element minOccurs="0" name="moduleName" type="xs:string">
				<xs:annotation>
					<xs:documentation osd:id="complexType.ProcessDefinition.moduleName" />
					<xs:appinfo>
						<osd:otherFacets>
							<osd:constraintEnumeration class="com.orchestranetworks.workflow.impl.util.ModuleEnumerationConstraint" />
						</osd:otherFacets>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataContext">
				<xs:annotation>
					<xs:documentation osd:id="complexType.ProcessDefinition.dataContext" />
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="variable" type="VariableDeclarationForProcess">
							<xs:annotation>
								<xs:documentation osd:id="complexType.ProcessDefinition.dataContext.variable" />
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="notificationProcessCreation" osd:access="RW" osd:class="com.orchestranetworks.workflow.impl.definition.WfbNotificationProcessCreation">
				<xs:annotation>
					<xs:documentation osd:id="complexType.ProcessDefinition.notificationProcessCreation" />
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="profiles" type="xs:string">
							<xs:annotation>
								<xs:documentation osd:id="complexType.ProcessDefinition.notificationProcessCreation.profiles" />
								<xs:appinfo>
									<osd:otherFacets>
										<osd:constraintEnumeration class="com.orchestranetworks.workflow.impl.util.ProfileEnumerationConstraint">
											<includeRoles>true</includeRoles>
											<includesUser>true</includesUser>
										</osd:constraintEnumeration>
									</osd:otherFacets>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element minOccurs="0" name="notificationMailId" type="xs:string">
							<xs:annotation>
								<xs:appinfo>
									<osd:otherFacets>
										<osd:constraintEnumeration class="com.onwbp.base.text.bean.ConstraintEnumerationRefInExternalAdaptationWithLocalizedLabel">
											<adaptation>configuration</adaptation>
											<tablePath>/root/configuration/notificationMessage</tablePath>
											<pathToDocumentation>./documentation</pathToDocumentation>
											<displayKey>true</displayKey>
										</osd:constraintEnumeration>
									</osd:otherFacets>
								</xs:appinfo>
								<xs:documentation osd:id="complexType.ProcessDefinition.notificationProcessCreation.notificationMailId" />
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="notificationProcessTermination" osd:access="RW" osd:class="com.orchestranetworks.workflow.impl.definition.WfbNotificationProcessTermination">
				<xs:annotation>
					<xs:documentation osd:id="complexType.ProcessDefinition.notificationProcessTermination" />
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element minOccurs="0" name="notifyProcessCreator" type="xs:boolean">
							<xs:annotation>
								<xs:documentation osd:id="complexType.ProcessDefinition.notificationProcessTermination.notifyProcessCreator" />
							</xs:annotation>
						</xs:element>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="profiles" type="xs:string">
							<xs:annotation>
								<xs:documentation osd:id="complexType.ProcessDefinition.notificationProcessTermination.profiles" />
								<xs:appinfo>
									<osd:otherFacets>
										<osd:constraintEnumeration class="com.orchestranetworks.workflow.impl.util.ProfileEnumerationConstraint">
											<includeRoles>true</includeRoles>
											<includesUser>true</includesUser>
										</osd:constraintEnumeration>
									</osd:otherFacets>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element minOccurs="0" name="notificationMailId" type="xs:string">
							<xs:annotation>
								<xs:appinfo>
									<osd:otherFacets>
										<osd:constraintEnumeration class="com.onwbp.base.text.bean.ConstraintEnumerationRefInExternalAdaptationWithLocalizedLabel">
											<adaptation>configuration</adaptation>
											<tablePath>/root/configuration/notificationMessage</tablePath>
											<pathToDocumentation>./documentation</pathToDocumentation>
											<displayKey>true</displayKey>
										</osd:constraintEnumeration>
									</osd:otherFacets>
								</xs:appinfo>
								<xs:documentation osd:id="complexType.ProcessDefinition.notificationProcessTermination.notificationMailId" />
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="priority" minOccurs="0" type="xs:integer">
				<xs:annotation>
					<xs:documentation osd:id="complexType.ProcessDefinition.priority" />
					<xs:appinfo>
						<osd:otherFacets>
							<osd:constraintEnumeration class="com.orchestranetworks.workflow.impl.util.PriorityEnumerationConstraint" />
						</osd:otherFacets>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="quickLaunching" minOccurs="0" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation osd:id="complexType.ProcessDefinition.quickLaunching" />	
				</xs:annotation>
			</xs:element>
			<xs:element name="synchronousLaunching" minOccurs="0" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation osd:id="complexType.ProcessDefinition.synchronousLaunching" />	
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="1" minOccurs="0" name="workflowTrigger" type="xs:string">
				<xs:annotation>
					<xs:documentation osd:id="complexType.ProcessDefinition.workflowTrigger" />
					<xs:appinfo>
						<osd:otherFacets>
							<osd:constraintEnumeration class="com.orchestranetworks.workflow.impl.util.WorkflowTriggerBeanEnumerationConstraint" />
						</osd:otherFacets>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="permissionsOnInstances" type="PermissionsOnInstances" />
			<xs:element maxOccurs="1" minOccurs="0" name="actionPermissions" type="xs:string">
				<xs:annotation>
					<xs:documentation osd:id="complexType.ProcessDefinition.actionPermissions" />
					<xs:appinfo>
						<osd:otherFacets>
							<osd:constraintEnumeration class="com.orchestranetworks.workflow.impl.util.ActionPermissionsOnWorkflowEnumerationConstraint" />
						</osd:otherFacets>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ProcessDefinitionPublished">
		<xs:annotation>
			<xs:documentation osd:id="complexType.ProcessDefinitionPublished" />
			<xs:appinfo>
				<osd:table>
					<primaryKeys>/homeKey /adaptationReference</primaryKeys>
					<mayCreateRoot>never</mayCreateRoot>
					<mayCreateOverwriting>never</mayCreateOverwriting>
					<mayCreateOcculting>never</mayCreateOcculting>
					<mayCreateDeriving>never</mayCreateDeriving>
					<mayDuplicate>never</mayDuplicate>
					<mayDelete>never</mayDelete>
					<index name="state">/state</index>
					<index name="name">/name</index>
				</osd:table>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="dwfActions" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation osd:id="complexType.ProcessDefinitionPublished.dwfActions" />
					<xs:appinfo>
						<osd:function class="com.orchestranetworks.workflow.impl.execution.ui.views.DwfActionsValueFunction" />
						<osd:uiBean class="com.orchestranetworks.workflow.impl.execution.ui.views.DwfPublicationActionsBeanEditor"/>
						<osd:defaultView>
   							<hiddenInSearch>true</hiddenInSearch>
						</osd:defaultView>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="name" type="xs:string">
				<xs:annotation>
					<xs:appinfo>
						<osd:otherFacets>
							<osd:constraint class="com.orchestranetworks.workflow.impl.util.CheckPublicationName" />
						</osd:otherFacets>
					</xs:appinfo>
					<xs:documentation osd:id="complexType.ProcessDefinitionPublished.name" />
				</xs:annotation>
			</xs:element>
			<xs:element name="dwfPublicationLabel" type="xs:string">
				<xs:annotation>
					<xs:documentation osd:id="complexType.ProcessDefinitionPublished.dwfPublicationLabel" />
					<xs:appinfo>
						<osd:function class="com.orchestranetworks.workflow.impl.execution.ui.views.DwfAdaptationPrimaryKeyValueFunction" />
						<osd:otherFacets>
							<osd:constraintEnumeration class="com.orchestranetworks.workflow.impl.execution.ui.views.DwfPublicationLabelWithoutNameConstraintEnumeration"/>
						</osd:otherFacets>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="state">
				<xs:annotation>
					<xs:documentation osd:id="complexType.ProcessDefinitionPublished.state" />
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="available">
							<xs:annotation>
								<xs:documentation osd:id="complexType.ProcessDefinitionPublished.state.enumeration.available"/>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="unavailable">
							<xs:annotation>
								<xs:documentation osd:id="complexType.ProcessDefinitionPublished.state.enumeration.unavailable"/>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="dwfCreationDate" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation osd:id="complexType.ProcessDefinitionPublished.dwfCreationDate" />
					<xs:appinfo>
						<osd:function class="com.orchestranetworks.workflow.impl.execution.ui.views.DwfCreationDateValueFunction" />
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="dwfWorkflowModel" type="xs:string">
				<xs:annotation>
					<xs:documentation osd:id="complexType.ProcessDefinitionPublished.dwfWorkflowModel" />
					<xs:appinfo>
						<osd:function class="com.orchestranetworks.workflow.impl.execution.ui.views.DwfAdaptationPrimaryKeyValueFunction" />
						<osd:otherFacets>
							<osd:constraintEnumeration class="com.orchestranetworks.workflow.impl.execution.ui.views.DwfPublicationWorkflowModelConstraintEnumeration"/>
						</osd:otherFacets>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="homeKey" type="xs:string">
				<xs:annotation>
					<xs:documentation osd:id="complexType.ProcessDefinitionPublished.homeKey" />
				</xs:annotation>
			</xs:element>
			<xs:element name="parent" minOccurs="0" type="xs:string">
				<xs:annotation>
					<xs:documentation osd:id="complexType.ProcessDefinitionPublished.parent" />
					<xs:appinfo>
						<osd:otherFacets>
							<osd:tableRef>
								<tablePath>/root/processPublished</tablePath>
							</osd:tableRef>
						</osd:otherFacets>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="dwfSnapshotLabel" type="xs:string">
				<xs:annotation>
					<xs:documentation osd:id="complexType.ProcessDefinitionPublished.dwfSnapshotLabel" />
					<xs:appinfo>
						<osd:function class="com.orchestranetworks.workflow.impl.execution.ui.views.DwfAdaptationPrimaryKeyValueFunction" />
						<osd:otherFacets>
							<osd:constraintEnumeration class="com.orchestranetworks.workflow.impl.execution.ui.views.DwfPublicationSnapshotLabelConstraintEnumeration"/>
						</osd:otherFacets>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="dwfSnapshotDescription" type="xs:string">
				<xs:annotation>
					<xs:documentation osd:id="complexType.ProcessDefinitionPublished.dwfSnapshotDescription" />
					<xs:appinfo>
						<osd:function class="com.orchestranetworks.workflow.impl.execution.ui.views.DwfAdaptationPrimaryKeyValueFunction" />
						<osd:uiBean class="com.orchestranetworks.workflow.impl.execution.ui.views.DwfPublicationSnapshotDescriptionImplBeanEditor"/>
						<osd:otherFacets>
							<osd:constraintEnumeration class="com.orchestranetworks.workflow.impl.execution.ui.views.DwfPublicationSnapshotDescriptionConstraintEnumeration"/>
						</osd:otherFacets>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="adaptationReference" type="xs:string">
				<xs:annotation>
					<xs:documentation osd:id="complexType.ProcessDefinitionPublished.adaptationReference" />
				</xs:annotation>
			</xs:element>
			<xs:element name="userReference" type="xs:string">
				<xs:annotation>
					<xs:documentation osd:id="complexType.ProcessDefinitionPublished.userReference" />
						<xs:appinfo>
							<osd:otherFacets>
								<osd:constraintEnumeration class="com.orchestranetworks.workflow.impl.util.ProfileEnumerationConstraint">
									<includeRoles>false</includeRoles>
									<includesUser>true</includesUser>
								</osd:constraintEnumeration>
							</osd:otherFacets>
						</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="documentation" minOccurs="0" osd:access="RW" type="Documentation" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="VariableDeclarationForProcess" osd:class="com.orchestranetworks.workflow.impl.definition.WfbVariableDeclarationForProcess">
		<xs:sequence>
			<xs:element minOccurs="1" name="name" osd:checkNullInput="true" type="xs:Name">
				<xs:annotation>
					<xs:documentation osd:id="complexType.VariableDeclarationForProcess.name" />
					<xs:appinfo>
						<osd:otherFacets>
							<osd:constraint class="com.orchestranetworks.workflow.impl.util.DataContextUniqueNameConstraint"/>
						</osd:otherFacets>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="defaultValue" type="xs:string">
				<xs:annotation>
					<xs:documentation osd:id="complexType.VariableDeclarationForProcess.defaultValue" />
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="inputParameter" type="xs:boolean">
				<xs:annotation>
					<xs:documentation osd:id="complexType.VariableDeclarationForProcess.inputParameter" />
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="outputParameter" type="xs:boolean">
				<xs:annotation>
					<xs:documentation osd:id="complexType.VariableDeclarationForProcess.outputParameter" />
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PNode">
		<xs:annotation>
			<xs:documentation osd:id="complexType.PNode" />
			<xs:appinfo>
				<osd:table>
					<primaryKeys>/idInProcess</primaryKeys>
					<defaultLabel osd:class="com.orchestranetworks.workflow.impl.definition.ui.WfLabelRendererPNode" />
					<mayCreateRoot>never</mayCreateRoot>
					<mayCreateOverwriting>never</mayCreateOverwriting>
					<mayCreateOcculting>never</mayCreateOcculting>
					<mayCreateDeriving>never</mayCreateDeriving>
					<mayDuplicate>never</mayDuplicate>
					<mayDelete>never</mayDelete>
				</osd:table>
				<osd:trigger class="com.orchestranetworks.workflow.impl.definition.WfsPNodeTrigger" />
				<osd:otherFacets>
					<osd:constraint class="com.orchestranetworks.workflow.impl.util.CheckPNodeConstraint" />
				</osd:otherFacets>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element minOccurs="1" name="idInProcess" type="xs:int">
				<xs:annotation>
					<xs:documentation osd:id="complexType.PNode.idInProcess" />
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="documentation" osd:access="RW" type="Documentation" />
			<xs:element minOccurs="0" name="firstInProcess" type="xs:boolean" />
			<xs:element minOccurs="0" name="executionModeOfNextStep" type="xs:string">
				<xs:annotation>
					<xs:documentation osd:id="complexType.PNode.executionModeOfNextStep"/>
					<xs:appinfo>
						<osd:otherFacets>
							<osd:constraintEnumeration class="com.orchestranetworks.workflow.impl.util.ExecutionModeOfNextStepEnumerationConstraint"/>
						</osd:otherFacets>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="userTask" type="UserTaskDefinition" />
			<xs:element minOccurs="0" name="scriptTask" type="ScriptTaskDefinition" />
			<xs:element minOccurs="0" name="condition" type="Condition" />
			<xs:element minOccurs="0" name="waitTask" type="WaitTaskDefinition" />
			<xs:element minOccurs="0" name="subWorkflowsInvocation" type="SubWorkflowsInvocation" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PLink">
		<xs:annotation>
			<xs:documentation osd:id="complexType.PLink" />
			<xs:appinfo>
				<osd:table>
					<primaryKeys>/fromNode /toNode</primaryKeys>
					<mayCreateRoot>never</mayCreateRoot>
					<mayCreateOverwriting>always</mayCreateOverwriting>
					<mayCreateOcculting>never</mayCreateOcculting>
					<mayCreateDeriving>always</mayCreateDeriving>
					<mayDuplicate>never</mayDuplicate>
					<mayDelete>always</mayDelete>
				</osd:table>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="fromNode" type="xs:string">
				<xs:annotation>
					<xs:documentation osd:id="complexType.PLink.fromNode" />
					<xs:appinfo>
						<osd:otherFacets>
							<osd:tableRef>
								<tablePath>//definition/pnode</tablePath>
								<!-- <documentationPath>./documentation</documentationPath> -->
							</osd:tableRef>
						</osd:otherFacets>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="toNode" type="xs:string">
				<xs:annotation>
					<xs:documentation osd:id="complexType.PLink.toNode" />
					<xs:appinfo>
						<osd:otherFacets>
							<osd:tableRef>
								<tablePath>//definition/pnode</tablePath>
								<!-- <documentationPath>./documentation</documentationPath> -->
							</osd:tableRef>
						</osd:otherFacets>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="role">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="other" />
						<xs:enumeration value="ifTrue" />
						<xs:enumeration value="ifFalse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="UserTaskDefinition" osd:access="RW" osd:class="com.orchestranetworks.workflow.impl.definition.WfbUserTask">
		<xs:annotation>
			<xs:documentation osd:id="complexType.UserTaskDefinition" />
			<xs:appinfo>
				<osd:uiBean class="com.orchestranetworks.workflow.impl.definition.ui.WfUIBeanUserTaskDefinition" />
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="version" osd:category="Hidden" type="xs:int" />
			<xs:element minOccurs="0" name="offerPolicy" osd:class="com.orchestranetworks.workflow.impl.definition.WfbOfferPolicy">
				<xs:annotation>
					<xs:documentation osd:id="complexType.UserTaskDefinition.offerPolicy" />
					<xs:appinfo>
						<!--
							<osd:uiBean
							class="com.orchestranetworks.workflow.editor.UIAssignment"/>
						-->
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="profiles" type="xs:string">
							<xs:annotation>
								<xs:documentation osd:id="complexType.UserTaskDefinition.offerPolicy.profiles" />
								<xs:appinfo>
									<osd:otherFacets>
										<osd:constraintEnumeration class="com.orchestranetworks.workflow.impl.util.ProfileEnumerationConstraint">
											<includeRoles>true</includeRoles>
											<includesUser>true</includesUser>
										</osd:constraintEnumeration>
									</osd:otherFacets>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element minOccurs="0" name="offeredToNotificationMail" type="xs:string">
							<xs:annotation>
								<xs:appinfo>
									<osd:otherFacets>
										<osd:constraintEnumeration class="com.onwbp.base.text.bean.ConstraintEnumerationRefInExternalAdaptationWithLocalizedLabel">
											<adaptation>configuration</adaptation>
											<tablePath>/root/configuration/notificationMessage</tablePath>
											<pathToDocumentation>./documentation</pathToDocumentation>
											<displayKey>true</displayKey>
										</osd:constraintEnumeration>
									</osd:otherFacets>
								</xs:appinfo>
								<xs:documentation osd:id="complexType.UserTaskDefinition.offerPolicy.offeredToNotificationMail" />
							</xs:annotation>
						</xs:element>
						<xs:element minOccurs="0" name="allocatedToNotificationMail" type="xs:string">
							<xs:annotation>
								<xs:appinfo>
									<osd:otherFacets>
										<osd:constraintEnumeration class="com.onwbp.base.text.bean.ConstraintEnumerationRefInExternalAdaptationWithLocalizedLabel">
											<adaptation>configuration</adaptation>
											<tablePath>/root/configuration/notificationMessage</tablePath>
											<pathToDocumentation>./documentation</pathToDocumentation>
											<displayKey>true</displayKey>
										</osd:constraintEnumeration>
									</osd:otherFacets>
								</xs:appinfo>
								<xs:documentation osd:id="complexType.UserTaskDefinition.offerPolicy.allocatedToNotificationMail" />
							</xs:annotation>
						</xs:element>
						<xs:element minOccurs="0" name="deallocatedToNotificationMail" type="xs:string">
							<xs:annotation>
